{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "error"
    },
  "inbounds": [{
    "protocol":"socks",
    "port":9897,
    "settings":{
      "auth":"noauth",
      "network": "tcp,udp"
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    },
    {
    "protocol": "dokodemo-door",
    "port": 9896,
    "settings": {
      "auth":"noauth",
      "network": "tcp,udp",
      "followRedirect": true
      },
    "streamSettings": {
      "sockopt": {"tcpFastOpen": true }
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
      }
    }],
  "outbounds": [
    {
    "mux": {"enabled": true},
    "protocol": "vmess",
    "settings": {
      "vnext": [{
        "address": "xxx.xxx",
        "port": 443,
        "users": [{
          "id": "00000000-0000-0000-0000-000000000000",
          "level": 1,
          "alterId": 64,
          "security": "AES-128-GCM"
          }]
        }]
    },
    "streamSettings": {
      "network": "ws",
      "security": "tls",
      "tlsSettings": {
        "serverName": "xxx.xxx",
        "allowInsecure": false
        },
      "wsSettings": {
        "path": "/xxxx",
        "headers": {
          "Host": "xxx.xxx"
          }
        },
      "sockopt": {
        "mark": 255,
        "tcpFastOpen": true
        }
      }
    }
  ]
}